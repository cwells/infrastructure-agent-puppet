{
    "name": "newrelic_infra",
    "version": "0.0.3",
    "author": "New Relic, Inc.",
    "summary": "A Puppet module for installing New Relic Infrastructure Agent",
    "license": "https://github.com/newrelic/infrastructure-agent-puppet/LICENSE.md",
